{
    "name": "hschulz/php-network-stuff",
    "description": "PHP network stuff.",
    "type": "library",
    "keywords": ["php", "ipv4", "ipv6", "subnet", "network"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Hauke Schulz",
            "email": "hauke27@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Hschulz\\Network\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hschulz\\Network\\Tests\\": "Tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "scripts": {
        "test": "phpunit --bootstrap=./vendor/autoload.php --whitelist=./src/ --testdox ./Tests/"
    }
}
